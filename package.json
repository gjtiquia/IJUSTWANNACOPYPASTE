{
    "name": "IJUSTWANNACOPYPASTE",
    "version": "1.0.50",
    "scripts": {
        "dev": "concurrently 'bun run dev:server' 'bun run dev:tailwind'",
        "dev:server": "bun run --watch src/index.tsx",
        "dev:tailwind": "bunx @tailwindcss/cli -i ./src/input.css -o ./public/styles.css --watch"
    },
    "dependencies": {
        "@elysiajs/html": "^1.4.0",
        "@elysiajs/static": "^1.4.7",
        "elysia": "latest",
        "tailwindcss": "^4.1.18"
    },
    "devDependencies": {
        "@tailwindcss/cli": "^4.1.18",
        "bun-types": "latest",
        "concurrently": "^9.2.1",
        "prettier": "^3.8.1"
    },
    "module": "src/index.js",
    "trustedDependencies": [
        "@parcel/watcher"
    ]
}
